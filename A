#include <iostream>
#include <string>
using namespace std;

class Lab5A{

int main()
{
char ch;
cout<<"Do you have a driving permit (Y/N)? ";
cin>>ch;
if(ch=='Y')
{
cout<<"Do you have a commercial driving license (Y/N)? ";
cin>>ch;
if(ch=='Y')
cout<<"You are eligible to purchase";
else if(ch=='N')
cout<<"Commercial driving license is prerequisite to purchase a vehicle!";
}
else
{
cout<<"Driving permit is a prerequisite to purchase a vechicle!";
}



return 0;
}

